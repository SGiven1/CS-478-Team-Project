<Window x:Class="BlissEditor.InsertTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlissEditor"
        mc:Ignorable="d"
        Title="InsertTable" Height="178" Width="242">
    <Grid>
        <TextBox x:Name="colTextBox" HorizontalAlignment="Left" Margin="78,57,0,0" TextWrapping="Wrap" Text="5" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="rowsTextBox" HorizontalAlignment="Left" Margin="78,20,0,0" TextWrapping="Wrap" Text="5" VerticalAlignment="Top" Width="120"/>
        <Label Content="Rows" HorizontalAlignment="Left" Margin="28,16,0,0" VerticalAlignment="Top"/>
        <Label Content="Columns" HorizontalAlignment="Left" Margin="19,53,0,0" VerticalAlignment="Top"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="149,95,0,0" VerticalAlignment="Top" Click="CancelBtn_Click"/>
        <Button Content="Accept" HorizontalAlignment="Left" Margin="47,95,0,0" VerticalAlignment="Top" Click="AcceptTableBtn_Click"/>
    </Grid>
</Window>
